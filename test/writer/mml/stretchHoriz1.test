      , [ EOver
            False (EText TextNormal "maps to") (ESymbol Accent "\8594")
        ]
      ]
    , [ [ EText TextNormal "all in row" ]
      , [ EOver
            False (ESymbol Accent "\8594") (EText TextNormal "maps to")

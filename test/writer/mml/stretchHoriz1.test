    [ AlignCenter , AlignCenter , AlignCenter ]
    [ [ []
      , [ EText TextNormal "Stetching in base" ]
      , [ EText TextNormal "Stretching in script" ]
      ]
    , [ [ EText TextNormal "no mrows" ]
      , [ EOver
            False (ESymbol Accent "\8594") (EText TextNormal "maps to")

<<< native
[ EText TextNormal "Inline nested fracs\160"
, EFraction
    NormalFrac
    (ESuper (ENumber "1") (ESqrt (ENumber "6")))
    (EGrouped
       [ ESqrt (ENumber "2")
       , ESymbol Bin "+"
       , EFraction
           NormalFrac
           (ENumber "1")
           (EGrouped
              [ ESqrt (ENumber "3")
              , ESymbol Bin "+"
              , EFraction
                  NormalFrac
                  (ENumber "1")
                  (EGrouped
                     [ ESqrt (ENumber "4")
                     , ESymbol Bin "+"
                     , EFraction
                         NormalFrac
                         (ENumber "1")
                         (EGrouped
                            [ ESqrt (ENumber "5")
                            , ESymbol Bin "+"
                            , EFraction
                                NormalFrac
                                (ENumber "1")
                                (EGrouped
                                   [ ESqrt (ENumber "6") , ESymbol Bin "+" , EIdentifier "\8230" ])
                            ])
                     ])
              ])
       ])
, EText
    TextNormal
    "\160are script size at the first level and decrease to script script size for all more nested levels."
]

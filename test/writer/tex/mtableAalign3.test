            , ENumber "5"
            ]
        ]
      , [ EIdentifier "z" ]
      ]
    , [ [ ENumber "1" ]
      , [ ENumber "2" ]
      , [ EFraction
            NormalFrac
            (EGrouped [ EIdentifier "a" , ESymbol Bin "+" , ENumber "5" ])
            (ENumber "3")
        ]
      ]
    ]
]
>>> tex

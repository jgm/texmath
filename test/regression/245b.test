upright("^\\prime") x^(')\
upright("^\\dprime") x^('')\
upright("^\\trprime") x^(''')\
upright("^\\qprime") x^('''')\
upright("^\\backprime") x^prime.rev\
upright("^\\backdprime") x^prime.double.rev\
upright("^\\backtrprime") x^prime.triple.rev\
upright("^\\hyphenbullet") x^(⁃)\
